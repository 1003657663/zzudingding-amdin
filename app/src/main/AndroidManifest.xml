        <meta-data android:name="YUNBA_APPKEY" android:value="55d4a6cac910afe56a6b7f7a" />
        <service android:name="io.yunba.android.core.YunBaService"> </service>
        <receiver android:name="io.yunba.android.core.YunBaReceiver">
